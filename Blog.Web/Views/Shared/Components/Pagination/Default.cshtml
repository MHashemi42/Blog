@model (string PreviousDisabled, string NextDisabled, int CurrentPage)

<nav>
    <ul class="pagination">
        <li class="page-item @Model.PreviousDisabled">
            <a class="page-link"
               asp-route-pageNumber="@(Model.CurrentPage - 1)">Previous</a>
        </li>
        <li class="page-item @Model.NextDisabled">
            <a class="page-link"
               asp-route-pageNumber="@(Model.CurrentPage + 1)">Next</a>
        </li>
    </ul>
</nav>